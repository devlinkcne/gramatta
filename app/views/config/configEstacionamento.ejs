<html lang="pt-br">

<head>
  <meta charset="UTF-8" />

  <title>Portal de notícias</title>

  <!-- JQuery -->
  <script src="js/jquery-2.2.1.js"></script>

  <!-- bootstrap - link cdn -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css"
    integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous" />

  <!-- incluindo css -->
  <link href="css/style.css" rel="stylesheet" />

  <!-- favicon -->
  <link rel="shortcut icon" href="images/favicon.ico" />
</head>

<body>
  <nav class="navbar navbar-default navbar-static-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
          aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        <img src="images/logo.png" />
      </div>

      <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav navbar-right">
          <li>
            <a href="/">HOME</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>

  <div class="container">
    <% if(validacao.length > 0) {%>
    <div class="row">
      <div class="col-md-12">
        <div class="alert alert-danger">
          <strong>Atenção!</strong>
          <ul>
            <% for(var i = 0; i < validacao.length; i++) { %>
            <li><%= validacao[i].msg%></li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
    <%}%>
    <% if(success.length > 0) {%>
    <div class="row">
      <div class="col-md-12">
        <div class="alert alert-success">
          <strong>Atenção!</strong>
          <ul>
            <% for(var i = 0; i < success.length; i++) { %>
            <li><%= success[i].msg%></li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
    <%}%>

    <div class="row">
      <div class="col-md-12">
          <form action="/form_configEstacionamento/saveChanges" method="POST">
              <div class="form-group">
                  <label for="titulo">Nome Fantasia:</label>
                  <input type="text" class="form-control" id="nome_fantasia" name="nome_fantasia"
                  value = "<%= configEstacionamento.nome_fantasia %>">
              </div>

              <div class="form-group">
                  <label for="pwd">Razão Social:</label>
                  <input type="text" class="form-control" id="razao_social" name="razao_social"
                  value = "<%= configEstacionamento.razao_social %>">
              </div>

              <div class="form-group">
                  <label for="pwd">Endereço:</label>
                  <input type="text" class="form-control" id="endereco" name="endereco"
                  value = "<%= configEstacionamento.endereco %>">
              </div>

              <div class="form-group">
                  <label for="pwd">CNPJ:</label>
                  <input type="text" class="form-control" id="cnpj" name="cnpj"
                  value = "<%= configEstacionamento.cnpj %>">
              </div>

              <div class="form-group">
                  <label for="pwd">Inscrição Municipal:</label>
                  <input type="text" class="form-control" id="inscricao_municipal" name="inscricao_municipal"
                  value = "<%= configEstacionamento.inscricao_municipal %>">
              </div>

              <div class="form-group">
                <label for="pwd">Telefone:</label>
                <input type="text" class="form-control" id="telefone" name="telefone"
                  value = "<%= configEstacionamento.telefone %>">
              </div>

              <div class="form-group">
                <label for="pwd">Website:</label>
                <input type="text" class="form-control" id="website" name="website"
                value = "<%= configEstacionamento.website %>">
              </div>

              <div class="form-group">
                <label for="pwd">Email:</label>
                <input type="text" class="form-control" id="email" name="email"
                value = "<%= configEstacionamento.email %>">
              </div>

              <button type="submit" class="btn btn-default">Cadastrar</button>

          </form>
      </div>
</div>

  </div>

  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"
    integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS"
    crossorigin="anonymous"></script>

</body>

</html>